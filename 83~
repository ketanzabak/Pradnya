#include <stdio.h>
int fun(int arr[], int n)
{
    int x = arr[0],i;
    for ( i = 1; i < n; i++)
        x = x ^ arr[i];
    return x;
}

int main()
{
  int arr[] = {9, 12, 2, 11, 10, 9, 12, 10, 9, 11, 2};
  int n = sizeof(arr)/sizeof(arr[0]);
  printf("%d ",fun(arr,n));

  return 0;
}

$ a)0  $ b) 9  $ c) 12  $ d) 2 $2
